<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allabo.fyl.fyl_server.mapper.UserMapper">

    <select id="selectUserProfile" resultType="com.allabo.fyl.fyl_server.dto.UserDto">
        SELECT id, identity_number, phone_number
        FROM customer
        WHERE id = #{userId}
    </select>

</mapper>
